<h1></h1>
<div class="log">
  <div class="content1">
    <h2>Sign In Form</h2>
    <%= render "shared/flash_messages", flash: flash %> 
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

      <%=f.text_field :email , :placeholder => "EMAIL" , :onfocus => "this.value = '';" , :onblur => "if (this.value == '') {this.value = 'EMAIL' ;} " %>
   
      <%= f.password_field :password, autocomplete: "off"  , :placeholder => "PASSWORD", :onfocus => "this.value = '';" , :onblur => "if (this.value == '') {this.value = 'PASSWORD' ;} "   %>
      <!--  <%# if devise_mapping.rememberable? -%>
          <div class="field">
            <%#= f.check_box :remember_me %>
            <%#= f.label :remember_me %>
          </div>
        <%# end -%> -->
      <div class="button-row">
           <%= f.submit "Log in" , :value => "Sign In" ,:class => "sign-in"   %>
        <input type="reset" class="reset" value="Reset">
        <div class="clear"></div>
      </div>
    <% end %>
    <%= render "users/shared/links" %>
  </div>
  <div class="clear"></div>
</div>
<div class="footer">
